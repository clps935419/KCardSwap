# Backend API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:8080/api/v1

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_REDIRECT_URI=kcardswap://
EXPO_PUBLIC_APP_SCHEME=kcardswap

# App Configuration
EXPO_PUBLIC_APP_NAME=KCardSwap
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment (development, staging, production)
# In development mode, email/password login will be shown
# In production/staging mode, only Google login will be shown
EXPO_PUBLIC_ENV=development
